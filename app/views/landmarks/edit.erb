<h1>Edit <%= @landmark.name %></h1>
<form method="post" action="/landmarks/<%= @landmark.id %>">
	<input type="hidden" id="hidden" name="_method" value="patch">
	<label>Edit Name</label>
	<input type="text"id="name" name="landmark[name]" value="<%= @landmark.name %>">
	<br></br>
	<label>Edit Year Completed</label>
	<input type="text" id="year_completed" name="landmark[year_completed]" value="<%= @landmark.year_completed %>"> <%= @landmark.year_completed.to_s %>
	<br></br>

	<input type="submit" value="Edit Landmark">
</form>